                        {
                          "Id": 605,
                          "Name": "SqlQuery",
                          "Value": "UPDATE       app.ortomo_MijnTest\nSET                Vragen = Q1.Vragen, VragenBeantwoord = Q1.VragenBeantwoord, VragenOpenstaand = Q1.VragenOpenstaand\nFROM            (SELECT        TOP (1000) SUM(app.ortomo_MijnVragenlijst.Vragen) AS Vragen, SUM(app.ortomo_MijnVragenlijst.VragenBeantwoord) AS VragenBeantwoord, SUM(app.ortomo_MijnVragenlijst.VragenOpenstaand) \n                                                    AS VragenOpenstaand, app.ortomo_MijnVragenlijst.Id AS MijnTestId\n                          FROM            app.ortomo_MijnVragenlijst INNER JOIN\n                                                    app.ortomo_MijnTest AS ortomo_MijnTest_1 ON app.ortomo_MijnVragenlijst.MijnTest = ortomo_MijnTest_1.Id\n                          GROUP BY app.ortomo_MijnVragenlijst.Id) AS Q1 INNER JOIN\n                         app.ortomo_MijnTest ON Q1.MijnTestId = app.ortomo_MijnTest.Id",
